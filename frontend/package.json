{
  "name": "web-starter-kit",
  "version": "0.2.1",
  "private": true,
  "dependencies": {
    "bootstrap": "4.0.0",
    "lodash": "4.17.5",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-helmet": "5.2.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.1.1",
    "react-snap": "1.11.4",
    "redux": "3.7.2",
    "redux-mock-store": "1.5.1",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path ../.gitignore",
    "lint:js": "npm run lint:eslint -- . ",
    "postbuild": "react-snap",
    "start": "react-scripts start",
    "test": "npm run lint && npm run test:client:coverage",
    "test:client": "react-scripts test --env=jsdom",
    "test:client:coverage": "npm run test:client -- --coverage",
    "test:client:debug": "react-scripts --inspect-brk test --runInBand --env=jsdom"
  },
  "proxy": "http://localhost:8000/",
  "devDependencies": {
    "@types/prop-types": "15.5.2",
    "@types/react": "16.0.40",
    "@types/react-dom": "16.0.4",
    "@types/react-redux": "5.0.15",
    "chai": "4.1.2",
    "cross-env": "5.1.3",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.18.2",
    "eslint-config-google": "0.9.1",
    "eslint-plugin-react": "7.7.0",
    "fetch-mock": "6.0.1",
    "inspect-process": "0.5.0",
    "mocha": "5.0.1",
    "react-test-renderer": "16.2.0",
    "redux-devtools-extension": "2.13.2",
    "sinon": "4.4.2"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox"
    ]
  }
}
