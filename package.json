{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for new React projects",
  "main": "server/index.js",
  "repository": "git@github.com:socketwiz/react-boilerplate.git",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config setup/webpack/webpack.prod.js --color -p --progress",
    "generate": "plop --plopfile setup/generators/index.js",
    "lint:eslint": "eslint --ignore-path .gitignore -- .",
    "start": "cross-env NODE_ENV=development node server",
    "start:production": "yarn run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "test": "eslint index.js && echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ricky Nelson <rickyn@socketwiz.com>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "hapi": "^16.1.0",
    "inert": "^4.1.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.14.0",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-react": "^6.8.0",
    "node-sass": "^4.1.1",
    "plop": "^1.7.3",
    "postcss-loader": "^1.2.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0"
  }
}
