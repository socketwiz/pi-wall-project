{
  "name": "react-starter",
  "version": "0.0.1",
  "description": "Starter code for new React projects",
  "main": "server/index.js",
  "repository": "git@github.com:socketwiz/react-starter.git",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config setup/webpack/webpack.prod.js --color -p --progress",
    "build:clean": "rimraf ./public/js/*",
    "generate": "plop --plopfile setup/generators/index.js",
    "lint:eslint": "eslint --ignore-path .gitignore -- .",
    "start": "yarn run build:clean && cross-env NODE_ENV=development node server",
    "start:production": "yarn run build && yarn run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "test": "cross-env NODE_ENV=test karma start setup/testing/karma.conf.js --single-run",
    "test:watch": "yarn run test -- --auto-watch --no-single-run",
    "coveralls": "cat ./coverage/lcov/lcov.info | coveralls"
  },
  "author": "Ricky Nelson <rickyn@socketwiz.com>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "hapi": "^16.1.0",
    "inert": "^4.1.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.14.0",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "cheerio": "^0.22.0",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.2",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-react": "^6.8.0",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.1",
    "mocha": "^3.2.0",
    "ngrok": "^2.2.4",
    "node-sass": "^4.1.1",
    "plop": "^1.7.3",
    "postcss-loader": "^1.2.1",
    "react-addons-test-utils": "^15.4.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0"
  }
}
